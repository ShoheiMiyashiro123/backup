<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
 "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
 "http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<package name="com.internousdev.EC1.action" extends="struts-default">
		<action name="HomeAction" class="com.internousdev.EC1.action.HomeAction" method="execute">
			<result name="success">login.jsp</result>
		</action>

		<action name="LoginAction" class="com.internousdev.EC1.action.LoginAction" method="execute">
			<result name="success">search.jsp</result>
			<result name="error">login.jsp</result>
		</action>

		<action name="UserCreateAction" class="com.internousdev.EC1.action.UserCreateAction" method="execute">
			<result name="success">userCreate.jsp</result>
		</action>

		<action name="UserCreateConfirmAction" class="com.internousdev.EC1.action.UserCreateConfirmAction" method="execute">
			<result name="success">userCreateConfirm.jsp</result>
			<result name="error">userCreate.jsp</result>
		</action>

		<action name="UserCreateCompleteAction" class="com.internousdev.EC1.action.UserCreateCompleteAction" method="execute">
			<result name="success">userCreateComplete.jsp</result>
			<result name="error">home.jsp</result>
		</action>

		<action name="BuyItemAction" class="com.internousdev.EC1.action.BuyItemAction" method="execute">
			<result name="success">buyItem.jsp</result>
		</action>

		<action name="BuyItemConfirmAction" class="com.internousdev.EC1.action.BuyItemConfirmAction" method="execute">
			<result name="success">buyItemConfirm.jsp</result>
		</action>

		<action name="BuyItemCompleteAction" class="com.internousdev.EC1.action.BuyItemCompleteAction" method="execute">
			<result name="success">buyItemComplete.jsp</result>
			<result name="lack">buyItem.jsp</result>
		</action>

		<action name="MyPageAction" class="com.internousdev.EC1.action.MyPageAction" method="execute">
			<result name="success">myPage.jsp</result>
		</action>

		<action name="SearchAction" class="com.internousdev.EC1.action.SearchAction" method="execute">
			<result name="success">search.jsp</result>
		</action>

		<action name="CartAction" class="com.internousdev.EC1.action.CartAction" method="execute">
			<result name="success">cart.jsp</result>
			<result name="error">search.jsp</result>
		</action>

		<action name="GoHomeAction" class="com.internousdev.EC1.action.GoHomeAction" method="execute">
			<result name="success">home.jsp</result>
		</action>
	</package>
</struts>
